//斐波那契数列
//int ch(int b)
//{
//	if (b <= 2)
//		return 1;
//	else
//		return ch(b - 1) + ch(b - 2);
//	
//}
int ch(int b)
{
	int d = 1;
	int n = 1;
	for (int i = b; i >2; i--)
	{
		 if (b > 2)
		{
			n += d;
			d = n-d;
		}
	}
	return n;
}
int main()
{
	int a = 0;
	int count = 0;
	(void)scanf("%d", &a);
	count=ch(a);
	printf("%d", count);
	return 0;
}
