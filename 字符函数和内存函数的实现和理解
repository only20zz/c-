//4.strcmp and My_Strcmp      比较函数
//strcmp用法
//int main()
//{
//	char arr1[] = "xuziyeshidashabi";
//	char arr2[] = "liujunfengshidashabi";	                       
//	printf("%d\n",strcmp(arr1, arr2));     //>0
//	char arr3[] = "xuziyeshidashabi";
//	char arr4[] = "xuziyeshidashabi";
//	printf("%d\n", strcmp(arr3, arr4));     //=0
//	char arr5[] = "liujunfengshidashabi";
//	char arr6[] = "xuziyeshidashabi";
//	printf("%d\n", strcmp(arr5, arr6));     //<0
//	return 0;
//}
//My_Strcmp实现
//int strcmp ( const char * str1, const char * str2 );
//方法1
//int My_Strcmp(const char* str1, const char* str2)
//{
//     assert(str1 && str2);
//	while (*str1 == *str2 && *str1 != '\0' && *str2 != '\0')
//	{
//		str1++;
//		str2++;
//	}
//	if (*str1 > *str2)
// 		return 1;
//	else if (*str1 == *str2)
//		return 0;
//	else
//		return -1;
// }
//方法2
//int My_Strcmp(const char* str1, const char* str2)
//{
//     assert(str1 && str2);
//	while (*str1 == *str2 && *str1 && *str2)
//	{
//		str1++;
//		str2++;
//	}
//	return *str1 - *str2;
//}
//int main()
//{
//	char arr1[] = "xuziyeshidashabi";
//	char arr2[] = "liujunfengshidashabi";
//	printf("%d\n", My_Strcmp(arr1, arr2));     //>0
//	char arr3[] = "xuziyeshidashabi";
//	char arr4[] = "xuziyeshidashabi";
